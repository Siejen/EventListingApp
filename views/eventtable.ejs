<table>
		<% posts.forEach( function ( post ) { %>
			<tr>
				<td> <a href="<%= post.dataValues.eventURL %>"><%= post.dataValues.eventName %></a></td>	
				<td> <%= post.dataValues.eventLocation %> </td>
				<td> <%= post.dataValues.eventCityAndState %> </td>
				<td> <%= post.dataValues.eventDate %> </td>
				<td> <%= post.dataValues.eventTime %> </td>
				<td> <%= post.dataValues.eventCost %> </td>
				<td> <%= post.dataValues.user.firstname %> </td>
			</tr>
		<% }) %>
</table>
